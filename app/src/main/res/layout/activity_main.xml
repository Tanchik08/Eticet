<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    android:backgroundTint="#000000"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="38dp"
        android:text="@string/atick"
        android:textColor="@color/white"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button"
        android:layout_width="364dp"
        android:layout_height="59dp"
        android:layout_marginTop="37dp"
        android:backgroundTint="#206AA5"
        android:onClick="btnAstatick"
        android:text="Этикет это"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:strokeColor="@color/origen"
        app:strokeWidth="2dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="@string/vidatick"
        android:textColor="@color/white"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button" />

    <Button
        android:id="@+id/button7"
        android:layout_width="150dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="24dp"
        android:backgroundTint="#9066DA"
        android:onClick="btnPut"
        android:text="Этикет путешествий"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button5"
        app:strokeColor="@color/origen"
        app:strokeWidth="2dp"/>

    <Button
        android:id="@+id/button2"
        android:layout_width="150dp"
        android:layout_height="48dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="63dp"
        android:backgroundTint="#009688"
        android:onClick="btnGr"
        android:text="Общегражданский этикет"
        android:textSize="13sp"
        app:layout_constraintEnd_toStartOf="@+id/button3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:strokeColor="@color/origen"
        app:strokeWidth="2dp"/>

    <Button
        android:id="@+id/button12"
        android:layout_width="150dp"
        android:layout_height="48dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="63dp"
        android:backgroundTint="#946BDD"
        android:onClick="btnDip"
        android:text="Дипломатический этикет"
        android:textSize="13sp"
        app:layout_constraintEnd_toStartOf="@+id/button13"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button10"
        app:strokeColor="@color/origen"
        app:strokeWidth="2dp"/>

    <Button
        android:id="@+id/button13"
        android:layout_width="150dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="24dp"
        android:backgroundTint="#009688"
        android:onClick="btnKorp"
        android:text="Корпоративный этикет"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button11"
        app:strokeColor="@color/origen"
        app:strokeWidth="2dp"/>

    <Button
        android:id="@+id/button4"
        android:layout_width="150dp"
        android:layout_height="48dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="63dp"
        android:backgroundTint="#976DE3"
        android:onClick="btnPris"
        android:text="Присутственный этикет"
        android:textSize="13sp"
        app:layout_constraintEnd_toStartOf="@+id/button5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button2"
        app:strokeColor="@color/origen"
        app:strokeWidth="2dp"/>

    <Button
        android:id="@+id/button10"
        android:layout_width="150dp"
        android:layout_height="48dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="63dp"
        android:backgroundTint="#009688"
        android:onClick="btnBrak"
        android:text="Брачный этикет "
        android:textSize="13sp"
        app:layout_constraintEnd_toStartOf="@+id/button11"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button8"
        app:strokeColor="@color/origen"
        app:strokeWidth="2dp"/>

    <Button
        android:id="@+id/button6"
        android:layout_width="150dp"
        android:layout_height="48dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="63dp"
        android:backgroundTint="#009688"
        android:onClick="btnPech"
        android:text="Печальный этикет"
        android:textSize="13sp"
        app:layout_constraintEnd_toStartOf="@+id/button7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button4"
        app:strokeColor="@color/origen"
        app:strokeWidth="2dp"/>

    <Button
        android:id="@+id/button5"
        android:layout_width="150dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="24dp"
        android:backgroundTint="#009688"
        android:onClick="btnRel"
        android:text="Религиозный этикет"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        app:strokeColor="@color/origen"
        app:strokeWidth="2dp"/>

    <Button
        android:id="@+id/button9"
        android:layout_width="150dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="24dp"
        android:backgroundTint="#009688"
        android:onClick="btnGender"
        android:text="Гендерный этикет"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button7"
        app:strokeColor="@color/origen"
        app:strokeWidth="2dp"/>

    <Button
        android:id="@+id/button3"
        android:layout_width="150dp"
        android:layout_height="48dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="24dp"
        android:backgroundTint="#9E6FF1"
        android:onClick="btnSoc"
        android:text="Светский или социальный этикет"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:strokeColor="@color/origen"
        app:strokeWidth="2dp"/>

    <Button
        android:id="@+id/button11"
        android:layout_width="150dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="24dp"
        android:backgroundTint="#956CDD"
        android:onClick="btnDel"
        android:text="Деловой этикет"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button9"
        app:strokeColor="@color/origen"
        app:strokeWidth="2dp"/>

    <Button
        android:id="@+id/button8"
        android:layout_width="150dp"
        android:layout_height="48dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="63dp"
        android:backgroundTint="#9467E4"
        android:onClick="btnSport"
        android:text="Спортивный этикет"
        android:textSize="13sp"
        app:layout_constraintEnd_toStartOf="@+id/button9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button6"
        app:strokeColor="@color/origen"
        app:strokeWidth="2dp"/>

    <Button
        android:id="@+id/button14"
        android:layout_width="150dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="24dp"
        android:backgroundTint="#A07AE4"
        android:onClick="btnSer"
        android:text="Этикет сервиса"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button13"
        app:strokeColor="@color/origen"
        app:strokeWidth="2dp"/>

    <Button
        android:id="@+id/button15"
        android:layout_width="150dp"
        android:layout_height="48dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="63dp"
        android:backgroundTint="#009688"
        android:onClick="btnVoin"
        android:text="Воинский этикет"
        android:textSize="13sp"
        app:layout_constraintEnd_toStartOf="@+id/button14"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button12"
        app:strokeColor="@color/origen"
        app:strokeWidth="2dp" />

</androidx.constraintlayout.widget.ConstraintLayout>